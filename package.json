{
  "name": "interview",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "clean": "rimraf dist",
    "start": "pnpm run clean && pnpm run build && node dist/server.js",
    "start:prod": "NODE_ENV=production pnpm run start",
    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "prisma:db-push": "prisma db push --schema=src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "passport-google-oauth": "^2.0.0",
    "prisma": "^6.14.0",
    "snowflake-id": "^1.1.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}